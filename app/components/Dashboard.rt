<rt-require dependency="react-grid-layout" as="ReactGridLayout"/>

<div id="dashboard">
  <h1>Dashboard</h1>

  <section>

      <ReactGridLayout layout={this.state.layout} cols={12} rowHeight={100} class="layout">

        <figure rt-repeat="Widget in this.state.widgets" key={WidgetIndex+1} class="widget">
          <Widget />
        </figure>

      </ReactGridLayout>

  </section>

</div>
