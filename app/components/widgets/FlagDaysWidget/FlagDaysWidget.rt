<div id="news-widget" class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">Finnish Flag Days</h3>
    </div>

    <div class="panel-body">
      <ul>
        <li rt-repeat="flagday in this.state.flagDays">
          <b>{flagday.name}</b> <small>{flagday.date}</small>
          </p>{flagday.description}</p>
        </li>
      </ul>
    </div>
</div>
